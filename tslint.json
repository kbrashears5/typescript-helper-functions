{
    "extends": "tslint:recommended",
    "defaultSeverity": "warning",
    "rules": {
        "interface-name": [
            false
        ],
        "max-classes-per-file": false,
        "max-line-length": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    }
}